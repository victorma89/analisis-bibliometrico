    issn = {0166-4972},
    journal = {Technovation},
    keywords = {Artificial intelligence, SDGs, Patents, Word2Vec, Semantic analysis},
    pages = {103335},
    title = {Exploring the role of artificial intelligence in addressing sustainable development. A semantic analysis of AI patents},

    issn = {1939-1382},
    journal = {IEEE Transactions on Learning Technologies},
    keywords = {Artificial intelligence;Education;Surveys;Data mining;Market research;Systematics;Recommender systems;Artificial intelligence (AI) in education;e-learning;educational data mining (EDM);generative AI for education;intelligent tutoring systems (ITS);machine learning (ML) in education;personalized learning},

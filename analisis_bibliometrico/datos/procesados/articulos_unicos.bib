    year = {2024}
}

@article{CHUANG2025103846,
    abstract = {Recent breakthroughs in generative artificial intelligence (genAI) have shaken the field of language assessment in unprecedented ways. On one hand, large-scale testing companies and organizations have spearheaded AI-based assessment models, infusing genAI elements into various test development and quality management procedures. On the other hand, in universities, although the interests in incorporating AI-based elements in instructional programs have also been soaring, little discussion has been made regarding the affordances and affordability of AI-based assessment systems in local contexts, and the impact of AI on language assessment across contexts remains underexplored. This paper presents a systematic review of 77 articles published on genAI use in language testing and assessment, to synthesize assessment challenges, opportunities, and solutions involving genAI. We review issues of genAI in the domains of reliability, validity, fairness, and practicality in large-scale, local, and classroom-based assessments. Based on the findings from the systematic review, we identify principles and best practices of language assessment in the age of genAI. Additionally, we provide future directions for assessment research and practice related to genAI, with a focus on impact and sustainability of genAI in language assessment across contexts. We caution researchers and practitioners about the danger of following the trend of incorporating GenAI in an unmeasured and unselective manner.},
    author = {Ping-Lin Chuang and Xun Yan},
    doi = {https://doi.org/10.1016/j.system.2025.103846},
    issn = {0346-251X},
    journal = {System},
    keywords = {generative artificial intelligence, language testing and assessment, challenges and opportunities},
    pages = {103846},
    title = {Language assessment in the era of generative artificial intelligence: Opportunities, challenges, and future directions},

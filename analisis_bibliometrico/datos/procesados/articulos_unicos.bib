    journal = {International Dental Journal},
    keywords = {Artificial intelligence, Machine learning, Periodontal disease, Photograph, Gum disease, Gingivitis},
    number = {5},
    pages = {100883},
    title = {Artificial Intelligence in Detecting Periodontal Disease From Intraoral Photographs: A Systematic Review},
    url = {https://www.sciencedirect.com/science/article/pii/S0020653925001728},
    volume = {75},
    year = {2025}
}


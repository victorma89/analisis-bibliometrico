    title = {Generative artificial intelligence-oriented synthetic network: Toward Integrated Fine-Tuning and Inference When Generative Artificial Intelligence Meets Edge Intelligence in the Intelligent Internet of Vehicles},
    volume = {20},
    year = {2025}
}

@inbook{10880573,
    abstract = {Summary <p>The term &#x201c;artificial intelligence&#x201d; (AI) refers to a remarkable technology enabling computers to carry out activities consistent with human behavior. It was in 1956 that John McCarthy first used the term &#x201c;artificial intelligence,&#x201d; and since then, it has become an indispensable component of our everyday life. AI is now the most widely used source of computation, and its applications range from automated voice systems to personalized movie suggestions. On the other hand, these systems depend on intricate methods such as speech recognition, natural language processing, and predictive analytics. As the way we interact with technology continues to be revolutionized by AI, it is essential to consider the potential privacy and security risks that come with it. Specifically, gathering and utilizing sensitive patient information to train AI models has given rise to significant issues. Patients, healthcare providers, and software vendors can suffer serious repercussions due to any utilization or disclosure of patient information. As a result of these worries, this chapter investigates the development and incorporation of AI into our health and information systems, with a particular emphasis on protecting personal information and safety.</p>},

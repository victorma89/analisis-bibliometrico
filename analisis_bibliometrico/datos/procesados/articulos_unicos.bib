    month = {May},
    number = {},
    pages = {903-907},
    title = {A Systematic Review on Incorporation of Artificial Intelligence in Precision Healthcare},
    volume = {},
    year = {2025}
}

@inproceedings{11104880,
    abstract = {The development of artificial intelligence (AI), particularly generative AI tools, has significantly transformed higher education. This study examined the key challenges encountered by college lecturers in the AI environment and the practical solutions they offered for these challenges. Seventeen heads of departments or academic units from a teacher education college participated in this study. Data were collected through a video-recorded focus group interview and supplemented reports. Data were analyzed inductively using the constant comparison method to identify recurring themes and patterns across the participants. The findings revealed five central challenges faced by college lecturers: (1) Assessment integrity that includes difficulties in verifying student authenticity, increasing plagiarism risks, and diminishing academic originality; (2) AI adoption resistance characterized by reluctance or hesitancy in embracing AI technologies stemming from limited familiarity and a lack of confidence; (3) Pedagogical adaptation imperative that necessitates the revision of instructional methodologies and a redefinition of the lecturer's role in the context of AI integration; (4) Technological literacy disparities that refer to gaps in digital literacy among both faculty and students, compound by unequal familiarity with AI tools; and (5) Limited institutional resources that result in restricted access to advanced AI tools and insufficient professional development opportunities. In response to these challenges, the participants proposed several potential solutions: fostering transparent and ethical student utilization of AI tools, implementing alternative assessment strategies focused on personal and critical thinking skills, enhancing institutional support through ICT centers, offering structured training programs, and ensuring equitable access to AI platforms. So, colleges need to open dialogue and collaboration among faculty members to develop shared guidelines and best practices for the ethical and effective use of AI in academic settings. In addition, teaching should focus more on thinking critically and personal skills, which are especially essential in the AI environment.},
    author = {Ahlam, Anabousy and Huda, Shayeb and Hala, Habayib and Nimer, Baya'A and Wajeeh, Daher},
    booktitle = {2025 International Conference on Smart Learning Courses (SCME)},
    doi = {10.1109/SCME62582.2025.11104880},
    issn = {},
    keywords = {Training;Resistance;Ethics;Navigation;Generative AI;Plagiarism;Education;Artificial intelligence;Interviews;Guidelines;Artificial intelligence;Technology in education;Teaching challenges. Higher education},
    month = {July},
    number = {},
    pages = {23-29},
    title = {Navigating AI Adoption: Challenges and Suggested Solutions for College Lecturers},
    volume = {},
    year = {2025}
}

@inproceedings{11104909,
    abstract = {This study investigates the diverse practices and impacts of generative artificial intelligence (AI) in higher education, focusing on faculty usage of AI tools. Employing a rigorous mixed-methods approach, the research combines a structured quantitative questionnaire exploring multiple dimensions of AI integration including pedagogical, scientific, institutional, and ethical uses with a qualitative experimental phase where faculty engage with standardized AI prompts to generate and analyze AI-produced content. The study aims to map declared usage patterns, evaluate perceived effects on teaching effectiveness and workload, and analyze disciplinary and seniority-based variations in adoption. Results reveal higher engagement and satisfaction among early-career faculty and those in engineering disciplines, alongside stronger ethical awareness, while more experienced faculty and those in literature fields report lower adoption. Quantitative analyses include descriptive statistics, ANOVA, factor analysis, and regression modeling, while qualitative data undergo thematic coding and natural language processing to identify recurrent usage themes and assess response quality. This research contributes to an interdisciplinary framework combining educational sciences, technological and pedagogical analysis, offering actionable insights for policy development and responsible, equitable AI integration in academia.},
    author = {El Hassouni, Mahmoud and Fouad, Fadoua and El Bassim, Hafsa},
    booktitle = {2025 International Conference on Smart Learning Courses (SCME)},

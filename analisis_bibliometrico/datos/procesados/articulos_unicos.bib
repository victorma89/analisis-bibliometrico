    volume = {},
    year = {2024}
}

@inproceedings{10692611,
    abstract = {We propose a deblurring method that integrates a combination of Generative Adversarial Networks (GANs) and encoder-decoder models. Our approach offers higher efficiency and reduced training complexity compared to generative deblurring models which utilizing GANs as the backbone network. In this approach, we employ the encoder as the feature extraction module, and then passes the features to the GANs for deblurring, followed by image reconstruction using the decoder as the last module. To ensure image fidelity, skip connections are introduced between the encoder and decoder. Meanwhile, we introduce a targeted training methodology designed to mitigate fluctuations in network parameters, thereby maximizing the efficacy of the model’s training velocity. We additionally conducted comparative evaluations based on the GoPro and Kohler datasets, contrasting our model against previously proposed algorithms to approve its exceptional performance in deblurring efficacy and efficiency.},

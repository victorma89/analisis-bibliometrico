@article{10930751,
    abstract = {The rise of large language models (LLMs) has transformed natural language processing (NLP) and generative artificial intelligence (AI) applications. However, deploying these transformer-based models in resource-constrained environments poses a significant challenge due to their high computational and memory demands. To address this, we introduce in this article generative AI (GenAI) on the Edge, a comprehensive benchmarking dataset designed to evaluate the performance of LLMs deployed on edge devices. Leveraging a distributed testbed of Raspberry Pi 5 devices orchestrated with lightweight Kubernetes (K3s), the dataset captures a broad range of performance metrics essential for assessing the feasibility of local inference in constrained environments. These metrics include detailed measurements of throughput, inference latency, memory utilization, and computational efficiency, along with granular timing data for key stages of the inference pipeline—sample, prefill, and decode phases. We systematically evaluate LLMs of varying sizes under real-world deployment scenarios, with a particular emphasis on CPU-based edge platforms. By conducting multiple runs of conversation-based evaluations, GenAI on the Edge provides actionable insights into the tradeoffs between performance and resource efficiency, enabling better decision-making for LLM deployment in edge environments. IEEE SOCIETY/COUNCIL Communications Society (ComSoc) DATA TYPE/LOCATION Structured Text Data (CSV); Leeds, U.K. DATA DOI/PID 10.21227/7d08-8655},
    author = {Nezami, Zeinab and Hafeez, Maryam and Djemame, Karim and Zaidi, Syed Ali Raza and Xu, Jie},
    doi = {10.1109/IEEEDATA.2025.3552083},
    issn = {2995-4274},
    journal = {IEEE Data Descriptions},
    keywords = {Measurement;Benchmark testing;Throughput;Data models;Artificial intelligence;Performance evaluation;Accuracy;Hardware;Generative AI;Memory management;Benchmarking dataset;edge artificial intelligence (AI);generative AI (GenAI);large language model (LLM);open radio access network (ORAN);performance evaluation},
    month = {},
    number = {},
    pages = {50-55},
    title = {Descriptor: Benchmark Dataset for Generative AI on Edge Devices (BeDGED)},
    volume = {2},
    year = {2025}
}

@inproceedings{10931912,
    abstract = {Web Scraping involves the use of bots for the purpose of extracting data from the online web. To extract such data, the web scraper must conduct at least 3 different steps, i.e., collect the necessary links from the web, extract data using the provided links, and save the information in a neatly organized CSV file or database. However, traditional methods to accomplish these steps have faced many difficulties, such as anti-scraping measures and website changes. To tackle these difficulties and to accomplish the steps in a more rapid yet advanced way, Artificial Intelligence (AI) can be utilized. As of mid-2024, the rapid development and improvement of AI has resulted in its integration into many businesses and educational institutions. The current state of AI is remarkably advanced that it can be integrated in about any field and sector available. In this paper, the researcher defined the term Web Scraping, Artificial Intelligence, Natural Language Processing, and Machine Learning, explained the use of AI in Web Scraping including its importance and potential, and the understanding of ethical web scraping practices. This literature review indicated that the integrated use of AI improves the performance of Web Scraping compared to traditional methods.},
    author = {Ayuso, Enrique and Dumfeh Brogya, Maame Serwah and Kumar Ahlawat, Vinay and Sain, Mangal},
    booktitle = {2024 International Conference on Communication, Control, and Intelligent Systems (CCIS)},

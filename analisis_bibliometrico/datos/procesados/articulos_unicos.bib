@inproceedings{10002630,
    abstract = {The imbalanced dataset is a challenging issue in many classification tasks. Because it leads a machine learning algorithm to poor generalization and performance. The imbalanced dataset is characterized as having a huge difference between the number of samples that contain each class. Unfortunately, various resampling methods are proposed to solve this problem. In our work, we target enhancing the handling of the imbalanced dataset using a new oversampling technique based on generative adversarial neural networks. Our method is benchmarked against the widely used oversampling technique including the synthetic minority oversampling technique (SMOTE), random oversampling technique (ROS), and the adaptive synthetic sampling approach(ADSYN). Additionally, three machine learning algorithms are used for evaluation. The outcome of our experiments on a real-world credit card dataset shows the strong ability of the proposed solution against the competitive oversampling techniques to overcome the imbalanced problem in the European credit card dataset.},
    author = {El Kafhali, Said and Tayebi, Mohammed},
    booktitle = {2022 6th SLAAI International Conference on Artificial Intelligence (SLAAI-ICAI)},
    doi = {10.1109/SLAAI-ICAI56923.2022.10002630},
    issn = {},
    keywords = {Machine learning algorithms;Neural networks;Europe;Benchmark testing;Credit cards;Task analysis;Artificial intelligence;Imbalanced classification;oversampling techniques;generative adversarial neural networks},
    month = {Dec},
    number = {},
    pages = {1-5},
    title = {Generative Adversarial Neural Networks based Oversampling Technique for Imbalanced Credit Card Dataset},
    volume = {},
    year = {2022}
}

@article{10035305,
    abstract = {We are in a golden age of progress in the field of artificial intelligence (AI). Radiotherapy is perfectly suited to benefit from AI to enhance accuracy and efficiency due to its technology-intensive nature and direct human–machine interactions. While large amount of AI research have recently been published in the field of photon therapy, applications of AI specifically targeted for particle therapy remain scarcely investigated. There are two distinct differences between the photon therapy and particle therapy: 1) beam interaction physics (photons versus charged particles) and 2) beam delivery mode (e.g., IMRT/VMAT versus pencil beam scanning). Consequently, different strategies of AI deployment are required for these two radiotherapy modalities. In this article, we aim to present a comprehensive survey of recent literature exclusively focusing on AI-powered particle therapy. Six major aspects are included: 1) treatment planning; 2) dose calculation; 3) range and dose verification; 4) image guidance; 5) quality assurance; and 6) adaptive replanning. A number of perspectives, as well as potential challenges and common pitfalls, are also discussed.},
    author = {Peng, Hao and Wu, Chao and Nguyen, Dan and Schuemann, Jan and Mairani, Andrea and Pu, Yuehu and Jiang, Steve},
    doi = {10.1109/TRPMS.2023.3241102},
    issn = {2469-7303},
    journal = {IEEE Transactions on Radiation and Plasma Medical Sciences},
    keywords = {Artificial intelligence;Medical treatment;Computed tomography;Radiation therapy;Planning;Generative adversarial networks;Photonics;Adaptive radiotherapy;artificial intelligence (AI);cone-beam CT (CBCT);dose calculation;dose verification;image guidance;positron emission tomography (PET);proton therapy;quality assurance (QA);treatment planning},
    month = {March},
    number = {3},
    pages = {213-224},
    title = {Recent Advancements of Artificial Intelligence in Particle Therapy},
    volume = {7},
    year = {2023}
}


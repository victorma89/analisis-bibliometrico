    keywords = {RNA–ligand interactions, artificial intelligence, RNA–ligand complex modeling, binding site prediction},
    number = {6},
    pages = {104366},
    title = {Artificial intelligence for RNA–ligand interaction prediction: advances and prospects},
    url = {https://www.sciencedirect.com/science/article/pii/S1359644625000790},

number = {3},
pages = {207-209},
year = {2024},
note = {Dermatology and Artificial Intelligence},
issn = {0738-081X},
doi = {https://doi.org/10.1016/j.clindermatol.2023.12.014},
url = {https://www.sciencedirect.com/science/article/pii/S0738081X23002663},
